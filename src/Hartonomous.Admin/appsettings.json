{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AdminTelemetry": {
    "PollIntervalSeconds": 5
  },
  "AtomIngestion": {
    "QueueCapacity": 1000,
    "MaxConcurrency": 10
  },
  "Pipelines": {
    "EnableResilience": true,
    "MaxRetries": 3,
    "CircuitBreakerThreshold": 0.5,
    "TimeoutSeconds": 30
  },
  "FeatureFlags": {
    "UsePipelineArchitecture": false,
    "UseAtomIngestionWorker": false,
    "UseEnsembleInferencePipeline": false
  },
  "ConnectionStrings": {
    "HartonomousDb": "Server=localhost;Database=Hartonomous;Trusted_Connection=True;TrustServerCertificate=True"
  }
}
